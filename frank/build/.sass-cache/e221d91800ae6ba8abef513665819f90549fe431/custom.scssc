	#text-12 {
		width: auto; }
	#text-13 {
		width: auto; }
	#text-7.widget {
		@include columns(6);
		#footer_follow_list {
			@extend .reset-dimensions;
			list-style: none;
			li {
				@extend .reset-dimensions;
				float: left;
				margin-right: 20px;
				a:hover {
					background: $color_highlight; }
				a {
					display: block;
					border: 0;
					@extend .reset-dimensions;
					background: $color_light;
